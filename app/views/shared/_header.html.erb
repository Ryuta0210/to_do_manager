<div class="header">
  <div class="header_left">
    <%= link_to image_tag("TOP_logo.jpg", class:"top_logo"), "/" %>
  </div>
  <div class="header_middle">To Do Manager</div>
  <div class="header_right">
    <% if user_signed_in? %>
    <span class="user_name"><%= current_user.full_name %></span>
    <%= link_to "ログアウト",destroy_user_session_path, data: {turbo_method: :delete}, class: "logout"%>
    <%else%>
    <%= link_to "ログイン",new_user_session_path%>
    <%= link_to "ユーザー新規登録",new_user_registration_path%>
    <%end%>
  </div>
</div>